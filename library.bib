@inproceedings{Diehl2015,
  title = {Fast-Classifying, High-Accuracy Spiking Deep Networks through Weight and Threshold Balancing},
  booktitle = {2015 {{International Joint Conference}} on {{Neural Networks}} ({{IJCNN}})},
