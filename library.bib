}

@article{dAutume2019,
  title = {Episodic {{Memory}} in {{Lifelong Language Learning}}},
  author = {{d'Autume}, Cyprien de Masson and Ruder, Sebastian and Kong, Lingpeng and Yogatama, Dani},
